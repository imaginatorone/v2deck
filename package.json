  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ov3rk/VLESSDeck"
  },
  "license": "MIT",
  "author": "ov3rk",
